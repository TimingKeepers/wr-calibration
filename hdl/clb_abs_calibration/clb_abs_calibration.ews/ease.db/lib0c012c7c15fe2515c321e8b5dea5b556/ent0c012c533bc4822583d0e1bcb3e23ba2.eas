               "    end if;"
               "  end process;"
               ""
               "  eof_p1    <= snk_cyc_d0 and not snk_i.cyc;"
               "  snk_valid <= (snk_i.cyc and snk_i.stb and snk_i.we) and not stall_int;"
               ""
               "  p_pipeline : process (clk_sys_i, Rst)"
               "  begin  -- process"
               "    if rising_edge(clk_sys_i) then"
               "      if(Rst = '1') then"
               "        q_valid    <= '0';"
               "--        q_valid_p1 <= '0';"
               "        q_data     <= (others => '0');"
               "--        q_data_p1  <= (Others => '0');"
               "      else"
               ""
               "	  if(snk_valid = '1' and snk_i.adr = c_WRF_DATA) then"
               "          q_data     <= snk_i.dat;"
               "          q_valid    <= pcs_dreq_i;"
               "        else"
               "          q_valid    <= '0';"
               "        end if;"
               ""
               "        -- if stall_int = '0' then"
               "          -- q_data_p1  <= q_data;"
               "          -- q_valid_p1 <= q_valid;"
               "	    -- end if;"
               ""
               "		end if;"
               "    end if;"
               "  end process;"
               ""
               "--  stall_int <= (not (pcs_dreq_i) or (snk_i.cyc xor snk_cyc_d0));"
               "  stall_int <= not (pcs_dreq_i);"
               ""
               "  snk_out.stall <= stall_int;"
               ""
               "  snk_out.err <= '0';"
               "  snk_out.rty <= '0';"
               ""
               "  p_gen_ack : process(clk_sys_i, Rst)"
               "  begin"
               "    if(Rst = '1') then"
               "      snk_out.ack <= '0';"
               "    elsif rising_edge(clk_sys_i) then"
               "      snk_out.ack <= snk_valid;"
               "    end if;"
               "  end process;"
               "  "
               "  snk_o <= snk_out;"
