               "-- "
               "-- EASE/HDL end ----------------------------------------------------------------"
               ""
               "architecture rtl of ipmux_in is"
               ""
               "-- general signals"
               "  signal q_valid    : std_logic;"
               "--  signal q_valid_p1 : std_logic;"
               "--  signal q_valid_p2 : std_logic;"
               "  signal q_data     : std_logic_vector(15 downto 0);"
